<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>CF_RELEASE</name>
          <description>CF release name - eg. cf-157.yml</description>
          <defaultValue>cf-157.yml</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>CF_MANIFEST</name>
          <description>CF manifest name - eg. cf-154-core.yml</description>
          <defaultValue>cf-154-core.yml</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>STEMCELL_URL</name>
          <description>Stemcell url - from any http server (NOT HTTPS) accessible from Jenkins - eg. http://bosh-jenkins-artifacts.s3.amazonaws.com/bosh-stemcell/aws/bosh-stemcell-1975-aws-xen-ubuntu.tgz</description>
          <defaultValue>http://bosh-jenkins-artifacts.s3.amazonaws.com/bosh-stemcell/aws/bosh-stemcell-1975-aws-xen-ubuntu.tgz</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SPIFF_DIR</name>
          <description>Directory of files to merge with manifest, using spiff</description>
          <defaultValue>aws</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.0.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/FreightTrain/C3CI-private</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions>
      <hudson.plugins.git.extensions.impl.CleanCheckout/>
      <hudson.plugins.git.extensions.impl.SubmoduleOption>
        <disableSubmodules>false</disableSubmodules>
        <recursiveSubmodules>true</recursiveSubmodules>
      </hudson.plugins.git.extensions.impl.SubmoduleOption>
    </extensions>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!bash -e

GITHUB_HASH=&quot;${GIT_URL}/tree/${GIT_COMMIT}&quot;
GITHUB_SPIFF_URL=&quot;${GITHUB_HASH}/cf/${SPIFF_DIR}&quot;
GITHUB_MANIFEST_URL=&quot;${GITHUB_HASH}/cf/${CF_MANIFEST}&quot;


echo &quot;***********************&quot;
export DESC=&quot;desc CF_RELEASE: ${CF_RELEASE} SPIFF_DIR: &lt;a href=&apos;${GITHUB_SPIFF_URL}&apos;&gt;SPIFF_URL&lt;/a&gt; CF_MANIFEST: &lt;a href=&apos;${GITHUB_MANIFEST_URL}&apos;&gt;GITHUB_MANIFEST_URL&lt;/a&gt; STEMCELL: &lt;a href=&apos;${STEMCELL_URL}&apos;&gt;STEMCELL&lt;/a&gt;&quot;
echo $DESC
echo &quot;DESC=$DESC&quot; &gt; tmp.var 
echo &quot;***********************&quot;

RUBY_VERSION=$(ruby -v | awk &apos;{print $2}&apos;)
export GEM_HOME=/var/vcap/store/gems/$RUBY_VERSION/$JOB_NAME
export GEM_PATH=$GEM_HOME
export PATH=$PATH:$GEM_HOME/bin

gem install bundler -v 1.3.5

(
cd C3CI
bundle
bundle exec rake cf:upload_and_deploy_release[\
`pwd`/cf-release/releases/${CF_RELEASE},\
`pwd`/../cf/${CF_MANIFEST},\
https://${BOSH_MICRO_IP}:25555,\
`pwd`/cf-release,\
${STEMCELL_URL},\
`pwd`/../cf/${SPIFF_DIR}\
]

)</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.descriptionsetter.DescriptionSetterPublisher plugin="description-setter@1.8">
      <regexp>desc CF_RELEASE: (.*) SPIFF_DIR: (.*) CF_MANIFEST: (.*) STEMCELL: (.*)</regexp>
      <regexpForFailed></regexpForFailed>
      <description>\1 &lt;br&gt; \2 &lt;br&gt; \3 &lt;br&gt; \4</description>
      <setForMatrix>false</setForMatrix>
    </hudson.plugins.descriptionsetter.DescriptionSetterPublisher>
    <hudson.plugins.parameterizedtrigger.BuildTrigger plugin="parameterized-trigger@2.22">
      <configs>
        <hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
          <configs>
            <hudson.plugins.parameterizedtrigger.FileBuildParameters>
              <propertiesFile>tmp.var</propertiesFile>
              <failTriggerOnMissing>false</failTriggerOnMissing>
            </hudson.plugins.parameterizedtrigger.FileBuildParameters>
          </configs>
          <projects>test-cf</projects>
          <condition>SUCCESS</condition>
          <triggerWithNoParameters>false</triggerWithNoParameters>
        </hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
      </configs>
    </hudson.plugins.parameterizedtrigger.BuildTrigger>
  </publishers>
  <buildWrappers/>
</project>